{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "bunVersion": "1.x",
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        },
        {
            "source": "/docs",
            "destination": "/docs/index.html"
        },
        {
            "source": "/docs/:match*",
            "destination": "/docs/:match*"
        },
        {
            "source": "/docs/docs/:match*",
            "destination": "/docs/:match*"
        },
        {
            "source": "/((?!api|docs$|docs/).*)",
            "destination": "/index.html"
        }
    ]
}