{
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/docs",
            "destination": "/docs/index.html"
        },
        {
            "source": "/docs/:match*",
            "destination": "/docs/:match*"
        },
        {
            "source": "/((?!docs$|docs/).*)",
            "destination": "/index.html"
        }
    ]
}