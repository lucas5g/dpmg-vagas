{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        },
        {
            "source": "/docs",
            "destination": "/docs/index.html"
        },
        {
            "source": "/docs/:match*",
            "destination": "/docs/:match*"
        },
        {
            "source": "/((?!api|docs$|docs/).*)",
            "destination": "/index.html"
        }
    ]
}